			function editLabel(labelid, from){
				if (from == '1') {
					var obj = document.getElementById('name_'+labelid);
					if (obj!=null) obj.style.display = 'none';
					obj = document.getElementById('color_'+labelid);
					if (obj!=null) obj.style.display = 'none';
